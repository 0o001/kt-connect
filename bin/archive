mkdir -p output/ktctl
docker run -v $PWD:$TRAVIS_BUILD_DIR --rm --entrypoint cp registry.cn-hangzhou.aliyuncs.com/rdc-incubator/kt-connect -r /go/src/github.com/alibaba/kt-connect/output/ktctl/* $TRAVIS_BUILD_DIR/output/ktctl/
ls -al $TRAVIS_BUILD_DIR/output/ktctl/

cd $TRAVIS_BUILD_DIR/output/ktctl/
tar zcvf ktctl_darwin_amd64.tar.gz ktctl_darwin_amd64
tar zcvf ktctl_darwin_386.tar.gz ktctl_darwin_386
tar zcvf ktctl_linux_amd64.tar.gz ktctl_linux_amd64
tar zcvf ktctl_linux_386.tar.gz ktctl_linux_386
