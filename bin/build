echo "=================build kt connect============"
docker build -t registry.cn-hangzhou.aliyuncs.com/rdc-incubator/kt-connect -f docker/kt-connect/Dockerfile .
mkdir -p output/ktctl
echo "=================copy artifact from container==="
docker run -v $PWD:$TRAVIS_BUILD_DIR --rm --entrypoint cp registry.cn-hangzhou.aliyuncs.com/rdc-incubator/kt-connect -r /go/src/github.com/alibaba/kt-connect/output/ktctl $TRAVIS_BUILD_DIR/output/ktctl
ls -al $TRAVIS_BUILD_DIR/output/ktctl

echo "=================build shadow container==="
docker build -t registry.cn-hangzhou.aliyuncs.com/rdc-incubator/kt-connect-shadow -f docker/shadow/Dockerfile .